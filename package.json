{
  "name": "@popcorn/workspace",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn lerna run --parallel dev",
    "build": "yarn lerna run --parallel build",
    "app:build": "yarn lerna run --scope={@popcorn/app,@popcorn/hardhat} build",
    "grants:build": "yarn lerna run --scope={@popcorn/grants,@popcorn/hardhat} build",
    "app:start": "yarn lerna run --scope @popcorn/app start",
    "start": "yarn lerna run --scope @popcorn/app start",
    "pretify": "prettier --write .",
    "test": "yarn lerna run test",
    "test:fork": "yarn lerna run test:fork",
    "test:forge": "yarn lerna run test:forge",
    "coverage": "yarn lerna run coverage",
    "netlify-build": "lerna run --scope @popcorn/hardhat compile && cd packages/app && yarn next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@netlify/plugin-nextjs": "3.9.2",
    "lerna": "3.22.1",
    "typescript": "4.5.4",
    "yarn": "1.22.17"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.0.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.2.1",
    "tsconfig-paths": "3.9.0"
  },
  "resolutions": {
    "@walletconnect/ethereum-provider": "1.7.1",
    "@web3-onboard/core": "2.2.7"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT"
}